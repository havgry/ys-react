{
  "name": "ys-react",
  "version": "0.0.2",
  "description": "React library for the YouSee design system",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest && codecov",
    "test:full": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "build": "webpack --mode production",
    "prebuild:storybook": "npm run test:generate-output",
    "storybook": "start-storybook -p 9001",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/havgry/ys-react.git"
  },
  "author": "Magnus Havgry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/havgry/ys-react/issues"
  },
  "homepage": "https://github.com/havgry/ys-react#readme",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|scss)$": "jest-transform-stub"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.13",
    "@storybook/addon-centered": "^4.1.13",
    "@storybook/addon-jest": "^4.1.13",
    "@storybook/addon-knobs": "^4.1.13",
    "@storybook/addon-storysource": "^4.1.13",
    "@storybook/react": "^4.1.13",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "codecov": "^3.2.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.1.0",
    "jest-transform-stub": "^2.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  }
}
